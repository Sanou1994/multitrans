                                {
                                    <center>
                                        <h2>Aucune transaction  </h2>
                                    </center>
                                }
                                else
                                {
                                    <table class="table align-items-center mb-0" id="tableID">
                                        <thead>
                                            <tr>
                                                <th>Agence </th>
                                                <th>Caissier(e)</th>
                                                <th>Frais</th>
                                                <th>Commission</th>
                                                <th>Encaissement</th>
                                                <th>Décaissement</th>
                                                <th>Surplux</th>
                                                <th>Taxe</th>
                                                <th>Dépense</th>
                                                <th>Extrat</th>
                                                <th>Date</th>
                                                <th>Détails</th>

