                                                    <td>@trans.dec </td>
                                                    <td>@trans.surplux </td>
                                                    <td>@trans.taxe </td>
                                                    <td class="bg-danger">@trans.depense </td>
                                                    <td>@trans.extrat </td>
                                                    <td>@date </td>
